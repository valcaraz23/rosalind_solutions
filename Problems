### CODE ID: DNA
s = ''

def base_count(string):
    return {
        s.count('A'),
        s.count('C'),
        s.count('G'),
        s.count('T')
    }

base_count(s)

### CODE ID: RNA
t=''
def transcribe(seq):
  u = seq.replace('T', 'U')
  return u
transcribe(t)

### CODE ID: REVC
s = ''
def rev_comp(seq):
  complement = {'A': 'T', 'T': 'A', 'C': 'G', 'G': 'C'}
  return "".join(complement.get(base, base) for base in reversed(seq))
rev_comp(s)

### ID: GC

from google.colab import files
rosalind_gc = files.upload()

def gc_content(seq):
  gc_percent = (seq.count('G')+seq.count('C'))/len(seq)*100
  return gc_percent

def highest_gc(fasta_data):
  sequences = {}
  current_id = None
  current_sequence_lines = []

for line in rosalind_gc.strip().split('\n'):
      if line.startswith('>'):
          if current_id is not None:
              sequences[current_id] = "".join(current_sequence_lines)
          current_id = line[1:] 
          current_sequence_lines = []
      else:
          current_sequence_lines.append(line.strip())

if current_id is not None:  # Add the last sequence
  sequences[current_id] = "".join(current_sequence_lines)

highest_gc_id = None
max_gc_content = 0.0

for seq_id, dna_sequence in sequences.items():
  gc_content = calculate_gc_content(dna_sequence)
  if gc_content > max_gc_content:
          max_gc_content = gc_content
          highest_gc_id = seq_id
return highest_gc_id, max_gc_content 



### ID: HAMM - Counting Point Mutations

s = ''
t = ''

if len(s) != len(t):
    raise ValueError
def hamming_dis(s,t):
  mismatch = 0
  for i in range(len(s)):
    if s[i] != t[i]:
      mismatch += 1
  return mismatch
hamming_dis(s,t)

## ID: PROT -  Translating RNA into Protein
code = {
    'UUU': 'F', 'UUC': 'F', 'UUA': 'L', 'UUG': 'L',
    'CUU': 'L', 'CUC': 'L', 'CUA': 'L', 'CUG': 'L',
    'AUU': 'I', 'AUC': 'I', 'AUA': 'I', 'AUG': 'M',
    'GUU': 'V', 'GUC': 'V', 'GUA': 'V', 'GUG': 'V',
    'UCU': 'S', 'UCC': 'S', 'UCA': 'S', 'TCG': 'S',
    'CCU': 'P', 'CCC': 'P', 'CCA': 'P', 'CCG': 'P',
    'ACU': 'T', 'ACC': 'T', 'ACA': 'T', 'ACG': 'T',
    'GCU': 'A', 'GCC': 'A', 'GCA': 'A', 'GCG': 'A',
    'UAU': 'Y', 'UAC': 'Y', 'UAA': '',  'UAG': '',
    'CAU': 'H', 'CAC': 'H', 'CAA': 'Q', 'CAG': 'Q',
    'AAU': 'N', 'AAC': 'N', 'AAA': 'K', 'AAG': 'K',
    'GAU': 'D', 'GAC': 'D', 'GAA': 'E', 'GAG': 'E',
    'UGU': 'C', 'UGC': 'C', 'UGA': '',  'UGG': 'W',
    'CGU': 'R', 'CGC': 'R', 'CGA': 'R', 'CGG': 'R',
    'AGU': 'S', 'AGC': 'S', 'AGA': 'R', 'AGG': 'R',
    'GGU': 'G', 'GGC': 'G', 'GGA': 'G', 'GGG': 'G', 'UCG':'S'
}

rna_seq = ''

def translate(seq):
  k=0
  prot = ''
  while k<len(seq)-2:
    codons = seq[k]+seq[k+1]+seq[k+2]
    k=k+3
    prot = prot + code[codons]
  return prot
   

translate(rna_seq)

## ID: SUBS - Finding a Motif in DNA
s = ''
t = 'GATAGCGGA'

def find_motif(seq, motif):
    locations = []
    start_index = 0
    while True:

        found_index = seq.find(motif, start_index)
       
        if found_index == -1:
            break
       
        locations.append(found_index + 1)
        
        start_index = found_index + 1 
        
    return locations
    
find_motif(s,t)