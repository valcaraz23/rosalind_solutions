### CODE ID: DNA - Counting DNA Nucleotides ########

s = ''

def base_count(string):
    return {
        s.count('A'),
        s.count('C'),
        s.count('G'),
        s.count('T')
    }

base_count(s)



### CODE ID: RNA - Transcribing DNA into RNA ######

t=''
def transcribe(seq):
  u = seq.replace('T', 'U')
  return u
transcribe(t)



### CODE ID: REVC -  Reverse Complement of a DNA Strand ########

s = ''
def rev_comp(seq):
  complement = {'A': 'T', 'T': 'A', 'C': 'G', 'G': 'C'}
  return "".join(complement.get(base, base) for base in reversed(seq))
rev_comp(s)



### ID: GC - Computing GC Content ########

def gc_content(sequence):
    gc_count = sequence.upper().count('G') + sequence.upper().count('C')
    total_bases = len(sequence)
    gc_percentage = (gc_count / total_bases) * 100
    return gc_percentage

def highest_gc(sequences):

    highest_gc_seq = None
    max_gc = -1  t

    for name, sequence in sequences.items():
        current_gc = gc_content(sequence)
        if current_gc> max_gc:
            highest_gc = current_gc
            highest_gc_sequence_name = name
            
    return highest_gc_sequence_name

dna_sequences = {
        'Rosalind_6406': 'CCTGCGGAAGATCGGCACTAGAATAGCCAGAACCGTTTCTCTGAGGCTTCCGGCCTTCCCTCCCACTAATAATTCTGAGG',
        'Rosalind_5959': 'CCATCGGTAGCGCATCCTTAGTCCAATTAAGTCCCTATCCAGGCGCTCCGCCGAAGGTCTATATCCATTTGTCAGCAGACACGC',
        'Rosalind_0808': 'CCACCCTCGTGGTATGGCTAGGCATTCAGGAACCGGAGAACGCTTCAGACCAGCCCGGACTGGGAACCTGCGGGCAGTAGGTGGAAT',
    }
  
highest_gc(dna_sequences)



### ID: HAMM - Counting Point Mutations ########

s = ''
t = ''

if len(s) != len(t):
    raise ValueError
def hamming_dis(s,t):
  mismatch = 0
  for i in range(len(s)):
    if s[i] != t[i]:
      mismatch += 1
  return mismatch
hamming_dis(s,t)



## ID: PROT -  Translating RNA into Protein ########

code = {
    'UUU': 'F', 'UUC': 'F', 'UUA': 'L', 'UUG': 'L',
    'CUU': 'L', 'CUC': 'L', 'CUA': 'L', 'CUG': 'L',
    'AUU': 'I', 'AUC': 'I', 'AUA': 'I', 'AUG': 'M',
    'GUU': 'V', 'GUC': 'V', 'GUA': 'V', 'GUG': 'V',
    'UCU': 'S', 'UCC': 'S', 'UCA': 'S', 'TCG': 'S',
    'CCU': 'P', 'CCC': 'P', 'CCA': 'P', 'CCG': 'P',
    'ACU': 'T', 'ACC': 'T', 'ACA': 'T', 'ACG': 'T',
    'GCU': 'A', 'GCC': 'A', 'GCA': 'A', 'GCG': 'A',
    'UAU': 'Y', 'UAC': 'Y', 'UAA': '',  'UAG': '',
    'CAU': 'H', 'CAC': 'H', 'CAA': 'Q', 'CAG': 'Q',
    'AAU': 'N', 'AAC': 'N', 'AAA': 'K', 'AAG': 'K',
    'GAU': 'D', 'GAC': 'D', 'GAA': 'E', 'GAG': 'E',
    'UGU': 'C', 'UGC': 'C', 'UGA': '',  'UGG': 'W',
    'CGU': 'R', 'CGC': 'R', 'CGA': 'R', 'CGG': 'R',
    'AGU': 'S', 'AGC': 'S', 'AGA': 'R', 'AGG': 'R',
    'GGU': 'G', 'GGC': 'G', 'GGA': 'G', 'GGG': 'G', 'UCG':'S'
}

rna_seq = ''

def translate(seq):
  k=0
  prot = ''
  while k<len(seq)-2:
    codons = seq[k]+seq[k+1]+seq[k+2]
    k=k+3
    prot = prot + code[codons]
  return prot
   

translate(rna_seq)



## ID: SUBS - Finding a Motif in DNA ########
s = ''
t = 'GATAGCGGA'

def find_motif(seq, motif):
    locations = []
    start_index = 0
    while True:

        found_index = seq.find(motif, start_index)
       
        if found_index == -1:
            break
       
        locations.append(found_index + 1)
        
        start_index = found_index + 1 
        
    return locations
    
find_motif(s,t)


## ID: PRTM - Calculating Protein Mass ########

monoisotopic_masses = {
    'A': 71.03711378,  # Alanine
    'R': 156.10111102, # Arginine
    'N': 114.04292744, # Asparagine
    'D': 115.02694302, # Aspartic acid
    'C': 103.00918478, # Cysteine
    'E': 129.04259309, # Glutamic acid
    'Q': 128.05857751, # Glutamine
    'G': 57.02146372,  # Glycine
    'H': 137.05891186, # Histidine
    'I': 113.08406398, # Isoleucine
    'L': 113.08406398, # Leucine
    'K': 128.09496301, # Lysine
    'M': 131.04048491, # Methionine
    'F': 147.06841391, # Phenylalanine
    'P': 97.05276385,  # Proline
    'S': 87.03202840,  # Serine
    'T': 101.04767847, # Threonine
    'W': 186.07931295, # Tryptophan
    'Y': 163.06332853, # Tyrosine
    'V': 99.06841391   # Valine
}

p='SKADYEK'

def total_weight(p):
  tot=0
  for aa in p:
    tot += monoisotopic_masses.get(aa, 0)
  return tot

total_weight(p)



## ID: REVP - Finding Reverse Palindromes ########

def REVP(dna_sequence, min_len=4, max_len=12):
    
    results = []
    n = len(dna_sequence)

    def REVC(seq):
        complement_map = {'A': 'T', 'T': 'A', 'C': 'G', 'G': 'C'}
        complement_seq = "".join([complement_map[base] for base in seq])
        return complement_seq[::-1]

    for i in range(n):
        for j in range(i + min_len, min(i + max_len + 1, n + 1)):
            substring = dna_sequence[i:j]
            if substring == REVC(substring):
                results.append((i + 1, len(substring)))

    return results

dna = "TCAATGCATGCGGGTCTATATGCAT"
REVP(dna)



#### ID: TRANS - Calculating Transition/Transversion Ratio ########

def R(s1, s2):
 
    transitions = 0
    transversions = 0

    purines = {'A', 'G'}
    pyrimidines = {'C', 'T'}

    for base1, base2 in zip(s1, s2):
        if base1 == base2:
            continue
      
        if base1 not in purines.union(pyrimidines) or base2 not in purines.union(pyrimidines):
            continue

        if (base1 in purines and base2 in purines) or (base1 in pyrimidines and base2 in pyrimidines):
            transitions += 1
        else:
            transversions += 1
 
    return transitions / transversions
  

seq1 = 'GCAACGCACAACGAAAACCCTTAGGGACTGGATTATTTCGTGATCGTTGTAGTTATTGGAAGTACGGGCATCAACCCAGTT'
seq2 = 'TTATCTGACAAAGAAAGCCGTCAACGGCTGGATAATTTCGCGATCGTGCTGGTTACTGGCGGTACGAGTGTTCCTTTGGGT'

R(seq1,seq2)
